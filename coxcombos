-- Create the button and UI
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create a ScreenGui to hold the buttons
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TripoutGui"
screenGui.Parent = playerGui

-- Create the "Tripout" button
local tripoutButton = Instance.new("TextButton")
tripoutButton.Name = "TripoutButton"
tripoutButton.Size = UDim2.new(0, 150, 0, 40)  -- Smaller button size (width, height)
tripoutButton.Position = UDim2.new(1, -160, 0, 10)  -- Position the button in the top-right corner
tripoutButton.Text = "Tripout"
tripoutButton.TextSize = 20  -- Smaller text size
tripoutButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text
tripoutButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Black background
tripoutButton.BackgroundTransparency = 0.3  -- Slight transparency to the background
tripoutButton.Parent = screenGui

