-- Function to stop any playing animations
local function stopAnimations()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass('Humanoid')

    if humanoid then
        -- Stop all animations that are currently playing
        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end

        -- Optionally, re-enable the Animate script if you want animations to resume
        character.Animate.Disabled = false
    else
        warn("Humanoid not found in character.")
    end
end

-- Connect the button clicks to the respective functions
tripoutButton.MouseButton1Click:Connect(playAnimations)
gokuButton.MouseButton1Click:Connect(playgoku)
shootButton.MouseButton1Click:Connect(playShootAnim)
reloadgunButton.MouseButton1Click:Connect(playreloadgun)
hostageButton.MouseButton1Click:Connect(playhostage)
ohyeah4Button.MouseButton1Click:Connect(playohyeah4)
sleepButton.MouseButton1Click:Connect(playsleep)
stopAnimButton.MouseButton1Click:Connect(stopAnimations)
heliButton.MouseButton1Click:Connect(playheli)
